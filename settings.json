{
    "editor.fontFamily": "Fira Code",
    "terminal.integrated.fontFamily": "Fira Code, MesloLGS NF",
    "editor.fontSize": 16, 
    "workbench.colorTheme": "Predawn",
    "workbench.activityBar.location": "default",
    "workbench.sideBar.location": "right",
    "terminal.integrated.fontWeightBold": "normal",
    "workbench.editorAssociations": {
        "*.mp4": "analyticsignal.preview-mp4"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "accessibility.signals.terminalBell": {
        "sound": "off"
    },
    "accessibility.signals.terminalCommandSucceeded": {
        "sound": "off"
    },
    "accessibility.signals.terminalCommandFailed": {
        "sound": "off"
    },
    "editor.accessibilitySupport": "on",
    "accessibility.signals.chatResponseReceived": {
        "sound": "off"
    },
    "accessibility.signals.taskFailed": {
        "announcement": "off",
        "sound": "off"
    },
    "accessibility.signalOptions.volume": 0,
    "terminal.integrated.inheritEnv": false,
    "codeium.enableConfig": {
        "*": false
    },
    "git.openRepositoryInParentFolders": "always",
    "makefile.configureOnOpen": true,
    "jupyter.askForKernelRestart": false,
    "python.defaultInterpreterPath": "/opt/homebrew/Caskroom/miniforge/base/envs/ml",
    "mssql.connectionGroups": [
        {
            "name": "ROOT",
            "id": "3E618445-BBF9-48C0-A7C4-806B3604C694"
        }
    ],
    "mssql.connections": [],
    "redhat.telemetry.enabled": false,
    "git.confirmSync": false,
    "python.analysis.languageServerMode": "light",
    "python.analysis.typeCheckingMode": "off",
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "explorer.fileNesting.patterns": {
        "*.ts": "${capture}.js",
        "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
        "*.jsx": "${capture}.js",
        "*.tsx": "${capture}.ts",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
        "Cargo.toml": "Cargo.lock",
        "*.sqlite": "${capture}.${extname}-*",
        "*.db": "${capture}.${extname}-*",
        "*.sqlite3": "${capture}.${extname}-*",
        "*.db3": "${capture}.${extname}-*",
        "*.sdb": "${capture}.${extname}-*",
        "*.s3db": "${capture}.${extname}-*"
    },
    "python.analysis.inlayHints.callArgumentNames": "partial",
    "python.analysis.diagnosticMode": "workspace",
    "python.languageServer": "Jedi",
    "mypy-type-checker.preferDaemon": true,
    "mypy-type-checker.reportingScope": "workspace",
    "terminal.external.osxExec": "iterm.app",
    "terminal.integrated.accessibleViewPreserveCursorPosition": true,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontLigatures.enabled": true,
    "terminal.integrated.lineHeight": 1.2
}
